# .env.example — коммитируемый шаблон (не содержит реальных секретов)
APP_ENV=development
APP_PORT=8080

# DATABASE_URL format: postgres://user:pass@host:port/dbname?sslmode=disable
DATABASE_URL=postgres://monitor:REPLACE_ME@postgres:5432/monitoring_db?sslmode=disable

# JWT secret - replace with a securely generated string when running locally / prod
JWT_SECRET=REPLACE_WITH_A_STRONG_SECRET

# Worker / monitoring
DEFAULT_INTERVAL_SECONDS=60
MIN_INTERVAL_SECONDS=30
WORKER_CONCURRENCY=5
HTTP_REQUEST_TIMEOUT_MS=5000

# Telegram (optional)
TELEGRAM_BOT_TOKEN=REPLACE_ME
TELEGRAM_CHAT_ID=REPLACE_ME

LOG_LEVEL=info
